	urldate = {2014-04-22},
	date = {2009-09-01},
	langid = {english},
	keywords = {new forms of power, performative infrastructures, post-hegemony, Scott Lash, social networking sites, social participation, technological unconscious, Web 2.0},
	file = {New Media Society-2009-Beer-985-1002.pdf:/Users/liam/Library/Application Support/Zotero/Profiles/dwy7wgsd.default/zotero/storage/2CHJ8QQC/New Media Society-2009-Beer-985-1002.pdf:application/pdf;Snapshot:/Users/liam/Library/Application Support/Zotero/Profiles/dwy7wgsd.default/zotero/storage/PKBWAX85/985.html:text/html}
