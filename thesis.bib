@article{lulka_animals_2014,
	title = {Animals, daguerreotypes and movement: The despair of fading and the emergence of ontology},
	volume = {19},
	issn = {1359-1835, 1460-3586},
	url = {http://mcu.sagepub.com.libproxy.mit.edu/content/19/1/35},
	doi = {10.1177/1359183513508032},
	shorttitle = {Animals, daguerreotypes and movement},
	abstract = {Animals are not well represented in daguerreotypes, yet their visage within them is quite informative. The inclusion of animals was inhibited by their penchant for movement, which confounded the capabilities of the daguerreotype. Nonetheless, people sought to incorporate animals into portraits, the result often being a smeared image of the being. Rather than viewing this outcome as a failure, the smeared images should be viewed as theoretically and philosophically insightful. On the one hand, the smears suggest the frailty of existence during an era in which disease and death were common, a condition that instigated the creation of lasting daguerreotypes. On the other hand, the smears indicate the transient character of life in all ages, a condition that points toward an altered conception of animal ontology. These social and ontological insights have ramifications for present-day relations.},
	pages = {35--58},
	number = {1},
	journaltitle = {Journal of Material Culture},
	shortjournal = {Journal of Material Culture},
	author = {Lulka, David},
	urldate = {2014-05-05},
	date = {2014-03-01},
	langid = {english},
	keywords = {Animals, daguerreotype, movement, representation, species concept},
	file = {Full Text PDF:/Users/liam/Library/Application Support/Zotero/Profiles/dwy7wgsd.default/zotero/storage/JZRVKXGB/Lulka - 2014 - Animals, daguerreotypes and movement The despair .pdf:application/pdf;Snapshot:/Users/liam/Library/Application Support/Zotero/Profiles/dwy7wgsd.default/zotero/storage/Q6Q22ZX7/35.html:text/html}
