@report{sompel_thoughts_????,
	title = {Thoughts on Referencing, Linking, Reference Rot},
	url = {http://mementoweb.org/missing-link/},
	abstract = {This document observes the ways in which web resources are formally cited in scholarly literature and Wikipedia articles. It further observes that temporal context information provided in citations, such as the date the cited resource was accessed or the {URI} of an archived version of that resource, is currently provided in a manner that is intended for human consumption. It then makes the case in favor of providing this information in a machine-actionable manner and proposes various approaches to do this in {HTML} documents. The proposed approaches can be used for formal citations but also for regular links. Including the temporal context information in the proposed ways, in combination with the use of existing archival infrastructure such as web archives and the Memento protocol, helps to ameliorate reference rot problems and can be considered a step towards increased web persistence.},
	author = {Sompel, Herbert Van de and Klein, Martin and Sanderson, Robert and Nelson, Michael},
	keywords = {hyperlinks, preservation}
}

@article{carriere_webquery:_1997,
	title = {{WebQuery}: searching and visualizing the Web through connectivity},
	volume = {29},
	issn = {0169-7552},
	url = {http://www.sciencedirect.com/science/article/pii/S0169755297000627},
	doi = {10.1016/S0169-7552(97)00062-7},
	series = {Papers from the Sixth International World Wide Web Conference},
	shorttitle = {{WebQuery}},
	abstract = {Finding information located somewhere on the World-Wide Web is an error-prone and frustrating task. The {WebQuery} system offers a powerful new method for searching the Web based on connectivity and content. We do this by examining links among the nodes returned in a keyword-based query. We then rank the nodes, giving the highest rank to the most highly connected nodes. By doing so, we are finding “hot spots” on the Web that contain information germane to a user's query. {WebQuery} not only ranks and filters the results of a Web query, it also extends the result set beyond what the search engine retrieves, by finding “interesting” sites that are highly connected to those sites returned by the original query. Even with {WebQuery} filtering and ranking query results, the result sets can be enormous. So, we need to visualize the returned information. We explore several techniques for visualizing this information—including cone trees, 2D graphs, 3D graphs, lists, and bullseyes-and discuss the criteria for using each of the techniques.},
	pages = {1257--1267},
	number = {8},
	journaltitle = {Computer Networks and {ISDN} Systems},
	shortjournal = {Computer Networks and {ISDN} Systems},
	author = {Carrière, S. Jeromy and Kazman, Rick},
	urldate = {2015-02-07},
	date = {1997-09},
	keywords = {information retrieval, Information visualization, visualization, World-Wide Web},
	file = {ScienceDirect Full Text PDF:/Users/liam/Library/Application Support/Zotero/Profiles/dwy7wgsd.default/zotero/storage/2T3QQHJA/Carrière and Kazman - 1997 - WebQuery searching and visualizing the Web throug.pdf:application/pdf;ScienceDirect Snapshot:/Users/liam/Library/Application Support/Zotero/Profiles/dwy7wgsd.default/zotero/storage/GJZEIBHQ/S0169755297000627.html:text/html}
