@article{moreira_health_2012,
	title = {Health Care Standards and the Politics of Singularities Shifting In and Out of Context},
	volume = {37},
	issn = {0162-2439, 1552-8251},
	url = {http://sth.sagepub.com.libproxy.mit.edu/content/37/4/307},
	doi = {10.1177/0162243911414921},
	abstract = {Context is a pivotal concept for social scientists in their attempt to weave singularities or universals to moral codes and political orders. However, in this, social scientists might be neglecting the ways in which individuals or groups who are excluded from the collective production of knowledge want to politicize their concerns also by claiming their uniqueness and singularity. In this article, drawing on the public controversy about access to dementia drugs on the U.K. National Health Service ({NHS}) and on the work of pioneering sociologist Helen {McGill} Hughes on “human interest stories,” the author argues that the “politics of singularities” can be articulated in two related ways within technical controversies. First, it expresses the unraveling of sociotechnical ties caused by institutional failure to take concerns into account. Second, it expresses the concrete uniqueness of persons caught by standardized, “universal” and impersonal implements and/or policies. Both these effects are underpinned by resourcing to allegorical expression, a literary form that while fostering political imagination in technological democracies might weaken Science and Technology Studies' ({STS}) ambitions to influence decision making.},
	pages = {307--331},
	number = {4},
	journaltitle = {Science, Technology \& Human Values},
	shortjournal = {Science Technology Human Values},
	author = {Moreira, Tiago},
	urldate = {2014-05-03},
	date = {2012-07-01},
	langid = {english},
	keywords = {allegory, Alzheimer’s disease, health care standards, human interest stories, politics of singularity},
	file = {Full Text PDF:/Users/liam/Library/Application Support/Zotero/Profiles/dwy7wgsd.default/zotero/storage/3XJ6GN9Z/Moreira - 2012 - Health Care Standards and the Politics of Singular.pdf:application/pdf;Snapshot:/Users/liam/Library/Application Support/Zotero/Profiles/dwy7wgsd.default/zotero/storage/GD3KPX7Z/4.html:text/html}
}

