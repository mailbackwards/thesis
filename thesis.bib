	note = {{\textless}p{\textgreater}Volume 40, Number 2, Spring 2006{\textless}/p{\textgreater}},
	file = {Attachment:/Users/liam/Library/Application Support/Zotero/Profiles/dwy7wgsd.default/zotero/storage/WAZJFK9B/Editing Archives] Archiving Editions - Dean Irvine.pdf:application/pdf;Project MUSE Full Text PDF:/Users/liam/Library/Application Support/Zotero/Profiles/dwy7wgsd.default/zotero/storage/BUK4PPXE/Irvine - 2006 - Editing Archives] Archiving Editions.pdf:application/pdf;Project MUSE Snapshot:/Users/liam/Library/Application Support/Zotero/Profiles/dwy7wgsd.default/zotero/storage/3A258QCJ/40.2irvine.html:text/html;Project MUSE Snapshot:/Users/liam/Library/Application Support/Zotero/Profiles/dwy7wgsd.default/zotero/storage/R6VT32F9/40.2irvine.html:text/html}
